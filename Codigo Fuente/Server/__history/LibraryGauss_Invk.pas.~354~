{ function AplicaCalificacion(const Calificacion: Double; const Faltas: Integer; const idmaterias: Integer; const idhistorialalumno: Integer; 
                                const idhistorialgrupo: Integer; const fecha: DateTime; const idtipocalificacion: Integer; const califAux2: Double; 
                                const califAux1: Double): Boolean; }
var
  Calificacion: Double;
  Faltas: Integer;
  idmaterias: Integer;
  idhistorialalumno: Integer;
  idhistorialgrupo: Integer;
  fecha: DateTime;
  idtipocalificacion: Integer;
  califAux2: Double;
  califAux1: Double;
  lResult: Boolean;
begin
  try
    __Message.Read('Calificacion', TypeInfo(Double), Calificacion, []);
    __Message.Read('Faltas', TypeInfo(Integer), Faltas, []);
    __Message.Read('idmaterias', TypeInfo(Integer), idmaterias, []);
    __Message.Read('idhistorialalumno', TypeInfo(Integer), idhistorialalumno, []);
    __Message.Read('idhistorialgrupo', TypeInfo(Integer), idhistorialgrupo, []);
    __Message.Read('fecha', TypeInfo(DateTime), fecha, [paIsDateTime]);
    __Message.Read('idtipocalificacion', TypeInfo(Integer), idtipocalificacion, []);
    __Message.Read('califAux2', TypeInfo(Double), califAux2, []);
    __Message.Read('califAux1', TypeInfo(Double), califAux1, []);

    lResult := (__Instance as IServiceGauss).AplicaCalificacion(Calificacion, Faltas, idmaterias, idhistorialalumno, idhistorialgrupo, fecha, idtipocalificacion, califAux2, califAux1);

    __Message.InitializeResponseMessage(__Transport, 'LibraryGauss', 'ServiceGauss', 'AplicaCalificacionResponse');
    __Message.Write('Result', TypeInfo(Boolean), lResult, []);
    __Message.Finalize;
    __Message.UnsetAttributes(__Transport);

  finally
  end;
end;

