  idSubtipoPago: Integer;
  status: AnsiString;
  idCiclos: Integer;
  idGrupo: Integer;
  lResult: Boolean;
begin
  try
    __Message.Read('idSubtipoPago', TypeInfo(Integer), idSubtipoPago, []);
    __Message.Read('status', TypeInfo(AnsiString), status, []);
    __Message.Read('idCiclos', TypeInfo(Integer), idCiclos, []);
    __Message.Read('idGrupo', TypeInfo(Integer), idGrupo, []);

    lResult := (__Instance as IServiceGauss).EliminaPagoCaja(idSubtipoPago, status, idCiclos, idGrupo);

    __Message.InitializeResponseMessage(__Transport, 'LibraryGauss', 'ServiceGauss', 'EliminaPagoCajaResponse');
    __Message.Write('Result', TypeInfo(Boolean), lResult, []);
    __Message.Finalize;
    __Message.UnsetAttributes(__Transport);

  finally
  end;
end;

