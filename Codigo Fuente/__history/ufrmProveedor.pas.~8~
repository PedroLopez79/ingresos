{$R *.dfm}

procedure TfrmProveedor.cdsCatalogoNewRecord(DataTable: TDADataTable);
begin
  inherited;
  cdsCatalogo.FieldByName('IDPROVEEDOR').AsInteger:= DM.Servidor.Folio('IDPROVEEDOR','');
end;

initialization
   ModuleInfoManager.RegisterModule('Proveedor', TfrmProveedor);

end.
