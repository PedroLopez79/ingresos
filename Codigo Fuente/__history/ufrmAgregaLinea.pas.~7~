{$R *.dfm}

function Abrir_AgregaLinea: Integer;
Begin
  ufrmAgregaLinea:=TufrmAgregaLinea.Create(Application);

  ufrmAgregaLinea.ShowModal;

  Result:= ufrmAgregaLinea.Ed_RFC.Text;
  ufrmAgregaLinea.Free;
End;

procedure TufrmAgregaLinea.Button2Click(Sender: TObject);
begin
  Close;
end;

