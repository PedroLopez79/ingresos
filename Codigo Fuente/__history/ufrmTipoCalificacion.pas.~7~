  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  frmTipoCalificacion: TfrmTipoCalificacion;

implementation

Uses uDM;

{$R *.dfm}

procedure TfrmTipoCalificacion.cdsCatalogoNewRecord(DataTable: TDADataTable);
begin
  inherited;
  cdsCatalogo.FieldByName('IDTIPOCALIFICACION').AsInteger:= DM.Servidor.Folio('IDTIPOCALIFICACION','');
end;

initialization
  ModuleInfoManager.RegisterModule('TipoCalificacion', TfrmTipoCalificacion);

end.
